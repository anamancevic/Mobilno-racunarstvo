<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".DashboardFragment">

    <androidx.cardview.widget.CardView
        android:id="@+id/cardIzlaz"
        android:layout_width="383dp"
        android:layout_height="72dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="488dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/textView8"
                android:layout_width="218dp"
                android:layout_height="27dp"
                android:layout_marginStart="16dp"
                android:text="@string/str_ext"
                android:textAppearance="@style/Theme.E_Biblioteka.TextBold"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageView
                android:id="@+id/imageViewIzlaz"
                android:layout_width="105dp"
                android:layout_height="72dp"
                android:layout_marginStart="28dp"
                app:layout_constraintStart_toEndOf="@+id/textView8"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/izlaz"
                tools:srcCompat="@drawable/izlaz" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/cardIznajmi"
        android:layout_width="383dp"
        android:layout_height="72dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="292dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/textView6"
                android:layout_width="218dp"
                android:layout_height="27dp"
                android:layout_marginStart="16dp"
                android:text="@string/str_rnt"
                android:textAppearance="@style/Theme.E_Biblioteka.TextBold"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageView
                android:id="@+id/imageViewIznajmi"
                android:layout_width="105dp"
                android:layout_height="72dp"
                android:layout_marginStart="28dp"
                android:visibility="visible"
                app:layout_constraintStart_toEndOf="@+id/textView6"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/knjiga_uzmi"
                tools:srcCompat="@drawable/knjiga_uzmi"
                tools:visibility="visible" />

        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.cardview.widget.CardView>

    <TextView
        android:id="@+id/twTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="132dp"
        android:layout_marginTop="224dp"
        android:text="@string/str_wcm"
        android:textAppearance="@style/Theme.E_Biblioteka.Heading"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.cardview.widget.CardView
        android:id="@+id/cardVrati"
        android:layout_width="383dp"
        android:layout_height="72dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="388dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/textView7"
                android:layout_width="218dp"
                android:layout_height="27dp"
                android:layout_marginStart="16dp"
                android:text="@string/str_ret"
                android:textAppearance="@style/Theme.E_Biblioteka.TextBold"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageView
                android:id="@+id/imageViewVrati"
                android:layout_width="105dp"
                android:layout_height="72dp"
                android:layout_marginStart="28dp"
                app:layout_constraintStart_toEndOf="@+id/textView7"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/knjiga_vrati"
                tools:srcCompat="@drawable/knjiga_vrati" />

        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.cardview.widget.CardView>

    <ImageView
        android:id="@+id/imageViewProfile"
        android:layout_width="149dp"
        android:layout_height="148dp"
        android:layout_marginStart="128dp"
        android:layout_marginTop="68dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/profil"
        tools:srcCompat="@drawable/profil" />

</androidx.constraintlayout.widget.ConstraintLayout>